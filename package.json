    "less-loader": "^4.1.0",
    "postcss-loader": "^2.1.2",
    "react-hot-loader": "^4.0.0",
    "rimraf": "^2.6.2",
    "style-loader": "^0.20.3",
    "webpack": "^4.1.1",
    "webpack-cli": "^2.0.12",
    "webpack-dev-server": "^3.1.1"
  }
